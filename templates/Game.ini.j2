; Special Settings for Ark Survival Evolved.
; Use these to make minor tweaks to your game/server.
; The settings in this file are the base for The Lagoverse Server
[/Script/ShooterGame.ShooterGameMode]
; If you want the game to run under "Single Player" Settings - Affects bosses etc
bUseSingleplayerSettings=false
; This allows spikeywalls etc to damage tames without rider (prevents kamikaze runs)
bPassiveDefensesDamageRiderlessDinos=true
; Prevents respawn spamming in PvP
bIncreasePvPRespawnInterval=true
; Reduces the radius in which resources don't respawn around players
ResourceNoReplenishRadiusPlayers=0.300000
; Reduces the radius in which resources don't respawn around buildings
ResourceNoReplenishRadiusStructures=0.300000
; Allows buildings of higher floors on a saddle platform
bAllowPlatformSaddleMultiFloors=true
; Respec as often as you want (Still needs a mindwipe though)
bAllowUnlimitedRespecs=true
; Don't use collision detection when building.
bDisableStructurePlacementCollision=true
; Better loot from Supply Drops
SupplyCrateLootQualityMultiplier=3.0
; Better loot from Fishing
FishingLootQualityMultiplier=3.0
; Allow flyer speed leveling
bAllowFlyerSpeedLeveling=true

; ---------------------------------------------------------------------------------------------------------------------
; Engram Settings
; ---------------------------------------------------------------------------------------------------------------------
bAutoUnlockAllEngrams=true ; Enable to automatically unlock all Engrams as people reach the appropriate level.

; ---------------------------------------------------------------------------------------------------------------------
; These Settings affect the mating and eggs of Dinosaurs, making the playable experience more enjoyable.
; By reducing these timers, or increasing multipliers, breeding dinos takes only hours instead of days.
; ---------------------------------------------------------------------------------------------------------------------
MatingIntervalMultiplier=0.003000
EggHatchSpeedMultiplier=40.000000
BabyMatureSpeedMultiplier=80.000000
BabyCuddleIntervalMultiplier=0.010000
BabyImprintAmountMultiplier=1.5


; ---------------------------------------------------------------------------------------------------------------------
; PvP and PvE Specific Settings
; ---------------------------------------------------------------------------------------------------------------------
; When enabled, PvE/PVP toggles with a timer.
bAutoPvETimer=false
; Place a shiny beacon where you died.
bUseCorpseLocator=true
; No Friendly Fire between tribe members and alliances.
bDisableFriendlyFire=true
bPvEDisableFriendlyFire=true

; ---------------------------------------------------------------------------------------------------------------------
; Genesis Specific Settings - These affect only the Genesis Map
; ---------------------------------------------------------------------------------------------------------------------
; This prevents people from building in mission zones (enabled by default)
bGenesisUseStructuresPreventionVolume=true

